{
	"name": "finalwork",
	"version": "1.0.0",
	"main": "dist/index.js",
	"scripts": {
		"audit": "npm audit",
		"dev": "ts-node-dev src/index.ts",
		"build": "nest build",
		"start": "node dist/main.js",
		"start:local": "nest start --watch -e\"node --env-file ./enviroments/local.env\"",
		"typeorm": "ts-node -r tsconfig-paths/register ./ormconfig.ts",
		"start:dev": "nest start --watch --env-file ./environments/local.env",
		"migration:generate": "ts-node --loader ts-node/esm ./node_modules/typeorm/cli.js migration:generate",
		"start:docker:db": "docker compose --env-file ./environments/local.env -f docker-compose.yaml up --build",
		"create:local:bucket": "ts-node ./src/scripts/create-local-bucket.ts",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"migration:create": "cross-var npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
		"migration:revert": "npm run typeorm -- migration:revert",
		"migration:run": "npm run typeorm -- migration:run"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.717.0",
		"@nestjs/common": "^10.4.15",
		"@nestjs/config": "^3.3.0",
		"@nestjs/core": "^10.4.15",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/mapped-types": "^2.0.6",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.4.15",
		"@nestjs/schedule": "^4.1.2",
		"@nestjs/swagger": "^8.1.0",
		"@nestjs/typeorm": "^10.0.2",
		"@sentry/nestjs": "^8.47.0",
		"@sentry/profiling-node": "^8.47.0",
		"@typescript-eslint/eslint-plugin": "^8.18.1",
		"axios": "^1.7.9",
		"bcrypt": "^5.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cross-var": "^1.1.0",
		"dist": "^0.1.2",
		"express": "^4.18.2",
		"ioredis": "^5.4.2",
		"jsonwebtoken": "^9.0.0",
		"multer": "^1.4.5-lts.1",
		"nest-cli": "^0.0.2",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"pg": "^8.13.1",
		"redis": "^4.7.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"typeorm": "^0.3.20",
		"uuid": "^11.0.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^4.17.14",
		"@types/jest": "^29.5.2",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/multer": "^1.4.12",
		"@types/node": "^20.10.0",
		"@types/supertest": "^6.0.0",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^9.17.0",
		"eslint-plugin-react": "^7.37.2",
		"globals": "^15.14.0",
		"jest": "^29.5.0",
		"lint-staged": "^15.2.2",
		"prettier": "^3.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.1"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
